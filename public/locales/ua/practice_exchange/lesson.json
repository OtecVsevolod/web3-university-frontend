{
  "practice_exchange_lesson1_title": "–£—Ä–æ–∫ 1: –†–µ—î—Å—Ç—Ä–∞—Ü—ñ—è –Ω–∞ –±—ñ—Ä–∂—ñ",
  "practice_exchange_lesson1_paragraph1": "üîπ <highlight>–ö—Ä–æ–∫ 1:</highlight> –ù–∞—Ç–∏—Å–Ω–∏ –Ω–∞ –ø–æ—Å–∏–ª–∞–Ω–Ω—è –±—ñ—Ä–∂—ñ ‚Äî <a href=\"https://www.bybit.com/invite?ref=B1XGLW\" target=\"_blank\" rel=\"noopener noreferrer\">https://www.bybit.com/invite?ref=B1XGLW</a>.",
  "practice_exchange_lesson1_paragraph2": "üîπ <highlight>–ö—Ä–æ–∫ 2:</highlight> –ü—ñ–¥—Ç–≤–µ—Ä–¥–∏, —â–æ —Ö–æ—á–µ—à –≤—ñ–¥–∫—Ä–∏—Ç–∏ –ø–æ—Å–∏–ª–∞–Ω–Ω—è —É —Å–ø–ª–∏–≤–∞—é—á–æ–º—É –≤—ñ–∫–Ω—ñ (–Ω–∞—Ç–∏—Å–Ω–∏ ¬´–¢–∞–∫¬ª).",
  "practice_exchange_lesson1_paragraph3": "üîπ <highlight>–ö—Ä–æ–∫ 3:</highlight> –í–≤–µ–¥–∏ —Å–≤—ñ–π email —Ç–∞ –ø—Ä–∏–¥—É–º–∞–π –Ω–∞–¥—ñ–π–Ω–∏–π –ø–∞—Ä–æ–ª—å.",
  "practice_exchange_lesson1_paragraph4": "üîπ <highlight>–ö—Ä–æ–∫ 4:</highlight> –ù–∞—Ç–∏—Å–Ω–∏ –∂–æ–≤—Ç—É –∫–Ω–æ–ø–∫—É <highlight>Claim Rewards</highlight>, —â–æ–± –æ—Ç—Ä–∏–º–∞—Ç–∏ –±–æ–Ω—É—Å–∏ —Ç–∞ –∑–Ω–∏–∂–∫–∏ –Ω–∞ –∫–æ–º—ñ—Å—ñ—ó.",
  "practice_exchange_lesson1_paragraph5": "üîπ <highlight>–ö—Ä–æ–∫ 5:</highlight> –ü—ñ–¥—Ç–≤–µ—Ä–¥–∏, —â–æ —Ç–∏ –Ω–µ —Ä–æ–±–æ—Ç ‚Äî –ø–µ—Ä–µ—Å—É–Ω—å –ø–∞–∑–ª –Ω–∞ —Å–≤–æ—î –º—ñ—Å—Ü–µ.",
  "practice_exchange_lesson1_paragraph6": "üîπ <highlight>–ö—Ä–æ–∫ 6:</highlight> –ü–µ—Ä–µ–≤—ñ—Ä –ø–æ—à—Ç—É ‚Äî —Ç—É–¥–∏ –ø—Ä–∏–π–¥–µ –∫–æ–¥ –ø—ñ–¥—Ç–≤–µ—Ä–¥–∂–µ–Ω–Ω—è –≤—ñ–¥ –±—ñ—Ä–∂—ñ.",
  "practice_exchange_lesson1_paragraph7": "üîπ <highlight>–ö—Ä–æ–∫ 7:</highlight> –í–≤–µ–¥–∏ –æ—Ç—Ä–∏–º–∞–Ω–∏–π –∫–æ–¥ –Ω–∞ —Å–∞–π—Ç—ñ –±—ñ—Ä–∂—ñ —Ç–∞ –∑–∞–≤–µ—Ä—à–∏ —Ä–µ—î—Å—Ç—Ä–∞—Ü—ñ—é.",
  "practice_exchange_lesson1_paragraph8": "üîπ <highlight>–ö—Ä–æ–∫ 8:</highlight> –ù–∞—Ç–∏—Å–Ω–∏ –∂–æ–≤—Ç—É –∫–Ω–æ–ø–∫—É <highlight>GET</highlight> (—Å—Ç—Ä—ñ–ª–∫–∞ 4 –Ω–∞ —Å–∫—Ä—ñ–Ω—à–æ—Ç—ñ) ‚Äî —Ç–µ–±–µ –ø–µ—Ä–µ–∫–∏–Ω–µ –≤ App Store / Google Play.",
  "practice_exchange_lesson1_paragraph9": "üîπ <highlight>–ö—Ä–æ–∫ 9:</highlight> –í—Å—Ç–∞–Ω–æ–≤–∏ –¥–æ–¥–∞—Ç–æ–∫ —ñ —É–≤—ñ–π–¥–∏ –∑ —Ç–∏–º —Å–∞–º–∏–º email —ñ –ø–∞—Ä–æ–ª–µ–º, —â–æ –≤–∏–∫–æ—Ä–∏—Å—Ç–æ–≤—É–≤–∞–≤(-–ª–∞) –ø—Ä–∏ —Ä–µ—î—Å—Ç—Ä–∞—Ü—ñ—ó.",
  "practice_exchange_lesson1_paragraph10": "üéâ –ì–æ—Ç–æ–≤–æ ‚Äî –∞–∫–∞—É–Ω—Ç —Å—Ç–≤–æ—Ä–µ–Ω–æ, –±–æ–Ω—É—Å–∏ –∞–∫—Ç–∏–≤–æ–≤–∞–Ω–æ, –º–æ–∂–Ω–∞ —Ä—É—Ö–∞—Ç–∏—Å—è –¥–∞–ª—ñ!",

  "practice_exchange_lesson2_title": "–£—Ä–æ–∫ 2: –ö—É–ø—ñ–≤–ª—è —Å—Ç–µ–π–±–ª–∫–æ—ó–Ω—ñ–≤ (P2P)",
  "practice_exchange_lesson2_paragraph1": "–£ —Ü—å–æ–º—É —É—Ä–æ—Ü—ñ –º–∏ –Ω–∞–≤—á–∏–º–æ—Å—è –ø–æ–ø–æ–≤–Ω—é–≤–∞—Ç–∏ –±–∞–ª–∞–Ω—Å —á–µ—Ä–µ–∑ P2P ‚Äî —Ü–µ <highlight>–ø—Ä—è–º–∞ –∫—É–ø—ñ–≤–ª—è –∫—Ä–∏–ø—Ç–æ–≤–∞–ª—é—Ç–∏ –≤ —ñ–Ω—à–∏—Ö –∫–æ—Ä–∏—Å—Ç—É–≤–∞—á—ñ–≤</highlight> –∑ –æ–ø–ª–∞—Ç–æ—é –±–∞–Ω–∫—ñ–≤—Å—å–∫–æ—é –∫–∞—Ä—Ç–∫–æ—é.",
  "practice_exchange_lesson2_paragraph2": "P2P (peer-to-peer) ‚Äî —Ü–µ —Å–ø–æ—Å—ñ–± –æ–±–º—ñ–Ω—É –±–µ–∑ –ø–æ—Å–µ—Ä–µ–¥–Ω–∏–∫—ñ–≤. –¢–∏ –∫—É–ø—É—î—à USDT –Ω–∞–ø—Ä—è–º—É –≤ —ñ–Ω—à–æ—ó –ª—é–¥–∏–Ω–∏. –ê <highlight>—Å—Ç–µ–π–±–ª–∫–æ—ó–Ω</highlight> ‚Äî —Ü–µ –∫—Ä–∏–ø—Ç–æ–≤–∞–ª—é—Ç–∞, –ø—Ä–∏–≤‚Äô—è–∑–∞–Ω–∞ –¥–æ –¥–æ–ª–∞—Ä–∞. –ù–∞–ø—Ä–∏–∫–ª–∞–¥, 1 USDT ‚âà 1 USD.",
  "practice_exchange_lesson2_paragraph3": "–ö—Ä–æ–∫ 1: –ù–∞—Ç–∏—Å–Ω–∏ –∫–Ω–æ–ø–∫—É <highlight>–ü—Ä–æ–π—Ç–∏</highlight>, —â–æ–± –ø—Ä–æ–π—Ç–∏ –≤–µ—Ä–∏—Ñ—ñ–∫–∞—Ü—ñ—é. –¶–µ –ø–æ—Ç—Ä—ñ–±–Ω–æ –¥–ª—è –±–µ–∑–ø–µ–∫–∏.",
  "practice_exchange_lesson2_paragraph4": "–ö—Ä–æ–∫ 2: –û–±–µ—Ä–∏ –≤–∫–ª–∞–¥–∫—É <highlight>P2P —Ç–æ—Ä–≥—ñ–≤–ª—è</highlight>. –°–∞–º–µ —Ç—É—Ç –≤—ñ–¥–±—É–≤–∞—é—Ç—å—Å—è —É–≥–æ–¥–∏.",
  "practice_exchange_lesson2_paragraph5": "–ö—Ä–æ–∫ 3: –ü–µ—Ä–µ–≤—ñ—Ä, —â–æ–± –±—É–ª–∞ –æ–±—Ä–∞–Ω–∞ –≤–∫–ª–∞–¥–∫–∞ <highlight>–ö—É–ø—ñ–≤–ª—è</highlight>, –∞ –≤–∞–ª—é—Ç–∞ ‚Äî <highlight>PLN</highlight> (–∞–±–æ —Ç–≤–æ—è –Ω–∞—Ü—ñ–æ–Ω–∞–ª—å–Ω–∞).",
  "practice_exchange_lesson2_paragraph6": "–ö—Ä–æ–∫ 4: –ü–µ—Ä–µ–∫–æ–Ω–∞–π—Å—è, —â–æ –æ–±—Ä–∞–Ω–æ <highlight>USDT</highlight> ‚Äî —Ü–µ —Ç–æ–π —Å–∞–º–∏–π —Å—Ç–µ–π–±–ª–∫–æ—ó–Ω, —â–æ –Ω–∞–º –ø–æ—Ç—Ä—ñ–±–µ–Ω.",
  "practice_exchange_lesson2_paragraph7": "–ö—Ä–æ–∫ 5: –ó–≤–µ—Ä–Ω–∏ —É–≤–∞–≥—É –Ω–∞ <highlight>–æ–±–º–µ–∂–µ–Ω–Ω—è –ø—Ä–æ–¥–∞–≤—Ü—è</highlight> ‚Äî –º—ñ–Ω—ñ–º–∞–ª—å–Ω—É —Ç–∞ –º–∞–∫—Å–∏–º–∞–ª—å–Ω—É —Å—É–º—É –ø–æ–∫—É–ø–∫–∏.",
  "practice_exchange_lesson2_paragraph8": "–ö—Ä–æ–∫ 6: –¢–∞–∫–æ–∂ –¥–∏–≤–∏—Å—å –Ω–∞ <highlight>—Ä–µ–π—Ç–∏–Ω–≥ –ø—Ä–æ–¥–∞–≤—Ü—è</highlight>. –ö—Ä–∞—â–µ –æ–±–∏—Ä–∞—Ç–∏ —Ç–∏—Ö, —É –∫–æ–≥–æ —Ä–µ–π—Ç–∏–Ω–≥ –ø–æ–Ω–∞–¥ 98%.",
  "practice_exchange_lesson2_paragraph9": "–î–∞–ª—ñ –Ω–∞—Ç–∏—Å–∫–∞—î—à –∫–Ω–æ–ø–∫—É <highlight>–ö—É–ø–∏—Ç–∏</highlight> —ñ –ø–µ—Ä–µ—Ö–æ–¥–∏—à –¥–æ –æ–ø–ª–∞—Ç–∏ ‚Äî –¥–æ—Ç—Ä–∏–º—É–π—Å—è —ñ–Ω—Å—Ç—Ä—É–∫—Ü—ñ–π, –æ–ø–ª–∞—Ç–∏ –∑ –∫–∞—Ä—Ç–∫–∏ –π –Ω–∞—Ç–∏—Å–Ω–∏ '—Å–ø–ª–∞—á–µ–Ω–æ'. –ü—ñ—Å–ª—è –ø—ñ–¥—Ç–≤–µ—Ä–¥–∂–µ–Ω–Ω—è –ø—Ä–æ–¥–∞–≤—Ü–µ–º ‚Äî —Ç–∏ –æ—Ç—Ä–∏–º–∞—î—à USDT –Ω–∞ –±–∞–ª–∞–Ω—Å –±—ñ—Ä–∂—ñ.",

  "practice_exchange_lesson3_title": "–£—Ä–æ–∫ 3: –û—Ñ–æ—Ä–º–ª–µ–Ω–Ω—è –æ—Ä–¥–µ—Ä–∞ —Ç–∞ –æ—Ç—Ä–∏–º–∞–Ω–Ω—è –∫—Ä–∏–ø—Ç–æ–≤–∞–ª—é—Ç–∏",
  "practice_exchange_lesson3_paragraph1": "–¢–µ–ø–µ—Ä —Ç–∏ –æ–±–∏—Ä–∞—î—à –∫–æ–Ω–∫—Ä–µ—Ç–Ω–æ–≥–æ –ø—Ä–æ–¥–∞–≤—Ü—è –∑—ñ —Å–ø–∏—Å–∫—É. –î–∏–≤–∏—Å—å –Ω–∞ –∫—É—Ä—Å, –ª—ñ–º—ñ—Ç–∏ —Ç–∞ –¥–æ—Å—Ç—É–ø–Ω—ñ —Å–ø–æ—Å–æ–±–∏ –æ–ø–ª–∞—Ç–∏. –ù–∞—Ç–∏—Å–Ω–∏ <highlight>¬´–ö—É–ø–∏—Ç–∏¬ª</highlight> –Ω–∞–≤–ø—Ä–æ—Ç–∏ –≤—ñ–¥–ø–æ–≤—ñ–¥–Ω–æ—ó –ø—Ä–æ–ø–æ–∑–∏—Ü—ñ—ó.",
  "practice_exchange_lesson3_paragraph2": "–í–≤–µ–¥–∏ –ø–æ—Ç—Ä—ñ–±–Ω—É —Å—É–º—É —É —Ñ—ñ–∞—Ç–Ω—ñ–π –≤–∞–ª—é—Ç—ñ (–Ω–∞–ø—Ä–∏–∫–ª–∞–¥, PLN), –ø–µ—Ä–µ–≤—ñ—Ä —Ñ—ñ–Ω–∞–ª—å–Ω—É —Å—É–º—É –≤ USDT. –ù–∞—Ç–∏—Å–Ω–∏ –∫–Ω–æ–ø–∫—É <highlight>¬´–ö—É–ø–∏—Ç–∏¬ª</highlight>.",
  "practice_exchange_lesson3_paragraph3": "–¢–µ–ø–µ—Ä —Ç–∏ –ø–æ–±–∞—á–∏—à –±–∞–Ω–∫—ñ–≤—Å—å–∫—ñ —Ä–µ–∫–≤—ñ–∑–∏—Ç–∏ –ø—Ä–æ–¥–∞–≤—Ü—è. –í—ñ–¥–∫—Ä–∏–π –∑–∞—Å—Ç–æ—Å—É–Ω–æ–∫ —Å–≤–æ–≥–æ –±–∞–Ω–∫—É —Ç–∞ –∑—Ä–æ–±–∏ –∑–≤–∏—á–∞–π–Ω–∏–π –ø–µ—Ä–µ–∫–∞–∑ –Ω–∞ —Ü—ñ —Ä–µ–∫–≤—ñ–∑–∏—Ç–∏. –£–í–ê–ì–ê: –Ω–µ –ø–∏—à–∏ –≤ –∫–æ–º–µ–Ω—Ç–∞—Ä—è—Ö —Å–ª–æ–≤–∞ –Ω–∞ –∫—à—Ç–∞–ª—Ç ¬´–∫—Ä–∏–ø—Ç–∞¬ª —á–∏ ¬´–±—ñ—Ç–∫–æ—ó–Ω¬ª.",
  "practice_exchange_lesson3_paragraph4": "–ü—ñ—Å–ª—è —Ç–æ–≥–æ —è–∫ —Ç–∏ –ø–µ—Ä–µ–∫–∞–∑–∞–≤ –≥—Ä–æ—à—ñ ‚Äî –ø–æ–≤–µ—Ä–Ω–∏—Å—å —É –≤—ñ–∫–Ω–æ —É–≥–æ–¥–∏ –π –ø–æ—Å—Ç–∞–≤ –≥–∞–ª–æ—á–∫–∏: ¬´–Ø –ø–µ—Ä–µ–∫–∞–∑–∞–≤¬ª, ¬´–Ø –æ–ø–ª–∞—á—É –∑ –≤–∫–∞–∑–∞–Ω–æ–≥–æ —Ä–∞—Ö—É–Ω–∫—É¬ª —Ç–∞ –Ω–∞—Ç–∏—Å–Ω–∏ –∫–Ω–æ–ø–∫—É <highlight>¬´–ü—ñ–¥—Ç–≤–µ—Ä–¥–∏—Ç–∏¬ª</highlight>.",
  "practice_exchange_lesson3_paragraph5": "–¢–µ–ø–µ—Ä –∑–∞–ª–∏—à–∏–ª–æ—Å—è —Ç—Ä–æ—Ö–∏ –∑–∞—á–µ–∫–∞—Ç–∏. –ü—Ä–æ–¥–∞–≤–µ—Ü—å –ø–µ—Ä–µ–≤—ñ—Ä–∏—Ç—å –ø–ª–∞—Ç—ñ–∂ —ñ –Ω–∞–¥—ñ—à–ª–µ —Ç–æ–±—ñ USDT. –ó–∞–∑–≤–∏—á–∞–π —Ü–µ –∑–∞–π–º–∞—î 2‚Äì10 —Ö–≤–∏–ª–∏–Ω. –ì–æ—Ç–æ–≤–æ ‚Äî –∫—Ä–∏–ø—Ç–∞ –≤–∂–µ –Ω–∞ —Ç–≤–æ—î–º—É –∞–∫–∞—É–Ω—Ç—ñ!",

  "practice_exchange_lesson4_title": "–£—Ä–æ–∫ 4: –î–µ —Ç–≤–æ—ó –º–æ–Ω–µ—Ç–∏ —ñ –Ω–∞–≤—ñ—â–æ –ø–æ—Ç—Ä—ñ–±–Ω—ñ —Ä—ñ–∑–Ω—ñ –∞–∫–∞—É–Ω—Ç–∏",
  "practice_exchange_lesson4_paragraph1": "–ü—ñ—Å–ª—è –ø–æ–∫—É–ø–∫–∏ –∫—Ä–∏–ø—Ç–∏ —á–µ—Ä–µ–∑ P2P –∞–±–æ –ø–æ–ø–æ–≤–Ω–µ–Ω–Ω—è –±–∞–ª–∞–Ω—Å—É —Ç–∏ –º–æ–∂–µ—à –∑–∞–¥—É–º–∞—Ç–∏—Å—è: <highlight>–¥–µ –º–æ—ó –º–æ–Ω–µ—Ç–∏?</highlight> –©–æ–± —ó—Ö –∑–Ω–∞–π—Ç–∏, –∑–∞–π–¥–∏ —É –≤–∫–ª–∞–¥–∫—É <highlight>¬´–ê–∫—Ç–∏–≤–∏¬ª</highlight> ‚Äî —Å–∞–º–µ —Ç–∞–º –ø–æ–∫–∞–∑—É—é—Ç—å—Å—è –≤—Å—ñ —Ç–≤–æ—ó –±–∞–ª–∞–Ω—Å–∏ –Ω–∞ –±—ñ—Ä–∂—ñ.",
  "practice_exchange_lesson4_paragraph2": "–£—Å–µ—Ä–µ–¥–∏–Ω—ñ ¬´–ê–∫—Ç–∏–≤—ñ–≤¬ª —Ç–∏ –ø–æ–±–∞—á–∏—à —Ç—Ä–∏ —Ä—ñ–∑–Ω—ñ —Ç–∏–ø–∏ –∞–∫–∞—É–Ω—Ç—ñ–≤. –ö–æ–∂–µ–Ω —ñ–∑ –Ω–∏—Ö –≤–∏–∫–æ–Ω—É—î —Å–≤–æ—é —Ñ—É–Ω–∫—Ü—ñ—é, —ñ <highlight>–ø–µ—Ä–µ–≤–µ–¥–µ–Ω–Ω—è –º—ñ–∂ –Ω–∏–º–∏</highlight> ‚Äî —Ü–µ –Ω–æ—Ä–º–∞–ª—å–Ω–∞ —Ç–∞ –ø–æ—Ç—Ä—ñ–±–Ω–∞ —á–∞—Å—Ç–∏–Ω–∞ —Ä–æ–±–æ—Ç–∏ –∑ –±—ñ—Ä–∂–µ—é.",
  "practice_exchange_lesson4_paragraph3": "1. <highlight>–§—ñ–Ω–∞–Ω—Å—É–≤–∞–Ω–Ω—è</highlight> ‚Äî —Ü–µ —Ç–≤—ñ–π –æ—Å–Ω–æ–≤–Ω–∏–π –≤—Ö—ñ–¥–Ω–∏–π –≥–∞–º–∞–Ω–µ—Ü—å. –°–∞–º–µ —Å—é–¥–∏ –ø—Ä–∏—Ö–æ–¥—è—Ç—å –º–æ–Ω–µ—Ç–∏ –ø—ñ—Å–ª—è –ø–æ–∫—É–ø–∫–∏ —á–µ—Ä–µ–∑ P2P, airdrop —á–∏ –æ—Ç—Ä–∏–º–∞–Ω–Ω—è –±–æ–Ω—É—Å—ñ–≤. –¶–µ –Ω–µ —Ç–æ—Ä–≥–æ–≤–∏–π —Ä–∞—Ö—É–Ω–æ–∫ ‚Äî —Ç–∏ –Ω–µ –∑–º–æ–∂–µ—à —Ç–æ—Ä–≥—É–≤–∞—Ç–∏ –º–æ–Ω–µ—Ç–∞–º–∏, –ø–æ–∫–∏ –Ω–µ –ø–µ—Ä–µ–≤–µ–¥–µ—à —ó—Ö –¥–∞–ª—ñ.",
  "practice_exchange_lesson4_paragraph4": "2. <highlight>–£–Ω—ñ—Ñ—ñ–∫–æ–≤–∞–Ω–∏–π —Ç–æ—Ä–≥–æ–≤–∏–π</highlight> ‚Äî —Ü–µ —Ç–≤—ñ–π –∞–∫—Ç–∏–≤–Ω–∏–π –±–∞–ª–∞–Ω—Å –¥–ª—è —Ç–æ—Ä–≥—ñ–≤–ª—ñ. –õ–∏—à–µ —Ç—É—Ç —Ç–∏ –º–æ–∂–µ—à –≤—ñ–¥–∫—Ä–∏–≤–∞—Ç–∏ –æ—Ä–¥–µ—Ä–∏ –Ω–∞ –∫—É–ø—ñ–≤–ª—é —á–∏ –ø—Ä–æ–¥–∞–∂ –∫—Ä–∏–ø—Ç–∏ –Ω–∞ —Å–ø–æ—Ç—ñ —á–∏ –¥–µ—Ä–∏–≤–∞—Ç–∏–≤–∞—Ö. –ü–µ—Ä–µ–¥ –ø–æ—á–∞—Ç–∫–æ–º —Ç–æ—Ä–≥—ñ–≤–ª—ñ –æ–±–æ–≤‚Äô—è–∑–∫–æ–≤–æ –ø–µ—Ä–µ–≤–æ–¥–∏ –º–æ–Ω–µ—Ç–∏ –∑ —Ñ—ñ–Ω–∞–Ω—Å—É–≤–∞–Ω–Ω—è —Å—é–¥–∏.",
  "practice_exchange_lesson4_paragraph5": "3. <highlight>Earn</highlight> ‚Äî —Ü–µ —Ä–∞—Ö—É–Ω–æ–∫ –¥–ª—è –ø–∞—Å–∏–≤–Ω–æ–≥–æ –¥–æ—Ö–æ–¥—É. –Ø–∫—â–æ —Ç–∏ –≤–∏—Ä—ñ—à–∏—à –∑–∞—Å—Ç–µ–π–∫–∞—Ç–∏ —Å–≤–æ—ó –º–æ–Ω–µ—Ç–∏ –∞–±–æ –±—Ä–∞—Ç–∏ —É—á–∞—Å—Ç—å —É –ø—É–ª—ñ –¥–æ—Ö–æ–¥—É (–Ω–∞–ø—Ä–∏–∫–ª–∞–¥, 5% –Ω–∞ —Ä—ñ–∫), —Ç–æ –º–æ–Ω–µ—Ç–∏ –∑–±–µ—Ä—ñ–≥–∞—Ç–∏–º—É—Ç—å—Å—è —Ç—É—Ç. –í–æ–Ω–∏ –ø—Ä–∞—Ü—é—é—Ç—å –Ω–∞ —Ç–µ–±–µ, –∞–ª–µ —Ç–∏–º—á–∞—Å–æ–≤–æ –Ω–µ–¥–æ—Å—Ç—É–ø–Ω—ñ –¥–ª—è —Ç–æ—Ä–≥—ñ–≤.",
  "practice_exchange_lesson4_paragraph6": "–©–æ–± –ø–µ—Ä–µ–≤–µ—Å—Ç–∏ –∫–æ—à—Ç–∏ –º—ñ–∂ –∞–∫–∞—É–Ω—Ç–∞–º–∏, –Ω–∞—Ç–∏—Å–Ω–∏ –∫–Ω–æ–ø–∫—É <highlight>¬´–ü–µ—Ä–µ–∫–∞–∑¬ª</highlight> –∑–≤–µ—Ä—Ö—É. –¶–µ –º–∏—Ç—Ç—î–≤–æ, –±–µ–∑–∫–æ—à—Ç–æ–≤–Ω–æ —Ç–∞ –Ω–µ –ø–æ—Ç—Ä–µ–±—É—î –ø—ñ–¥—Ç–≤–µ—Ä–¥–∂–µ–Ω–Ω—è.",
